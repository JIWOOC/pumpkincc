:284,"indent":0,"parameters":["오프닝4",false,false,0,0]},{"code":101,"indent":0,"parameters":["",0,1,2]},{"code":401,"indent":0,"parameters":["그래서 이야기의 첫 시작점으로 시간을 되돌렸어."]},{"code":224,"indent":0,"parameters":[[255,255,255,255],70,true]},{"code":221,"indent":0,"parameters":[]},{"code":230,"indent":0,"parameters":[60]},{"code":284,"indent":0,"parameters":["오프닝5",false,false,0,0]},{"code":222,"indent":0,"parameters":[]},{"code":284,"indent":0,"parameters":["오프닝5_1",false,false,0,0]},{"code":230,"indent":0,"parameters":[60]},{"code":284,"indent":0,"parameters":["오프닝5_2",false,false,0,0]},{"code":230,"indent":0,"parameters":[60]},{"code":284,"indent":0,"parameters":["오프닝5_3",false,false,0,0]},{"code":101,"indent":0,"parameters":["",0,1,2]},{"code":401,"indent":0,"parameters":["무도회 전에 예쁜 드레스와 호박마차를 만들어주었지."]},{"code":401,"indent":0,"parameters":["\\.유리구두도 선물해주고 말이야."]},{"code":250,"indent":0,"parameters":[{"name":"Book2","volume":50,"pitch":110,"pan":0}]},{"code":284,"indent":0,"parameters":["오프닝6",false,false,0,0]},{"code":101,"indent":0,"parameters":["",0,1,2]},{"code":401,"indent":0,"parameters":["그렇게 이야기의 흐름은 바뀌었고 아이는 행복해졌단다."]},{"code":221,"indent":0,"parameters":[]},{"code":230,"indent":0,"parameters":[60]},{"code":284,"indent":0,"parameters":["오프닝7",false,false,0,0]},{"code":222,"indent":0,"parameters":[]},{"code":101,"indent":0,"parameters":["",0,1,2]},{"code":401,"indent":0,"parameters":["그런데 새엄마와 언니들은 그 이후로 어떻게 되었는지 아니?"]},{"code":101,"indent":0,"parameters":["",0,1,2]},{"code":401,"indent":0,"parameters":["- 그러게요. 벌을 받았나요?"]},{"code":250,"indent":0,"parameters":[{"name":"Book2","volume":50,"pitch":110,"pan":0}]},{"code":284,"indent":0,"parameters":["오프닝8",false,false,0,0]},{"code":101,"indent":0,"parameters":["",0,1,2]},{"code":401,"indent":0,"parameters":["새엄마와 언니들은 행복해진 아이를 계속해서 질투하고 괴로워했어."]},{"code":101,"indent":0,"parameters":["",0,1,2]},{"code":401,"indent":0,"parameters":["지금까지 저지른 짓에 대해 반성을 하지도, 용서를 빌지도 않았지."]},{"code":401,"indent":0,"parameters":["버릇을 고치지 못하고 다른 곳에서도 똑같이 행동했단다."]},{"code":242,"indent":0,"parameters":[8]},{"code":250,"indent":0,"parameters":[{"name":"Book2","volume":50,"pitch":110,"pan":0}]},{"code":284,"indent":0,"parameters":["오프닝9",false,false,0,0]},{"code":101,"indent":0,"parameters":["",0,1,2]},{"code":401,"indent":0,"parameters":["그러다가 그 애에게 했던 만행이 퍼져서, 어떤 사교계에서도"]},{"code":401,"indent":0,"parameters":["그들을 받아주지 않았어. 결국 왕국에서 추방당했지."]},{"code":250,"indent":0,"parameters":[{"name":"Book2","volume":50,"pitch":110,"pan":0}]},{"code":284,"indent":0,"parameters":["오프닝10",false,false,0,0]},{"code":101,"indent":0,"parameters":["",0,1,2]},{"code":401,"indent":0,"parameters":["그 아이와 달리, 새엄마와 언니들은 영원히 행복해질 수 없었단다."]},{"code":230,"indent":0,"parameters":[60]},{"code":221,"indent":0,"parameters":[]},{"code":201,"indent":0,"parameters":[0,2,16,12,0,0]},{"code":121,"indent":0,"parameters":[1,1,0]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":0,"stepAnime":false,"through":false,"trigger":3,"walkAnime":true}],"x":16,"y":12}
